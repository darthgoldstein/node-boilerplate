{
  "name": "node-boilerplate",
  "version": "1.0.0",
  "description": "Node, express boilerplate",
  "main": "index.js",
  "scripts": {
    "build": "rimraf ./build && tsc -p ./tsconfig.json",
    "watch": "rm -rf ./build && tsc -p ./tsconfig.json --watch",
    "start": "node ./build/start.js"
  },
  "engines": {
    "node": "16.6.1",
    "npm": "7.20.5"
